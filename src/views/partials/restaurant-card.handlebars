<div class="col">
  <div class="card" style="min-height: 450px;">
    <img
      src={{logo}}
      class="card-img-top"
      alt="restaurant {{name}} card with information and image"
    />
    <div class="card-body">
      <h5 class="card-title">{{name}}</h5>
      <p class="card-text">{{trimDescription description}}</p>
    </div>
  </div>
</div>